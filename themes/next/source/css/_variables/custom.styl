// 标题，修改成你期望的字体族
$font-family-headings = SF-Mono-Bold, Georgia, sans
// 修改成你期望的字体族
$font-family-base = SF-Mono-Regular, "Microsoft YaHei", Verdana, sans-serif
// 代码字体
$code-font-family = SF-Mono-Regular, "Input Mono", "PT Mono", Consolas, Monaco, Menlo, monospace
// 正文字体的大小
$font-size-base = 16px
// 代码字体的大小
$code-font-size = 14px
// 代码块颜色
$code-foreground = #dd0055
// Background color for <body>
$body-bg-color = #e7e5dc  //theme mist use #fdfdfd
// text-color
$text-color = #353535
